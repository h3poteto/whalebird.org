<p id="notice" class="alert alert-dismissible alert-success"><%= notice %></p>

<div class="header">
  <h3><%= t('.title') %></h3>
</div>

<div class="contact-form">
  <p class="text-success"><%= t('.success') %></p>
  <br />
  <table class="table">
    <tr>
      <th><%= Inquiry.human_attribute_name(:email) %>:</th>
      <td><p><%= @inquiry.email %></p></td>
    </tr>
    <tr>
      <th><%= Inquiry.human_attribute_name(:body) %>:</th>
      <td><p><%= @inquiry.body %></p></td>
    </tr>
  </table>
</div>

